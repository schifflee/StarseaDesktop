          <Item id="4" index="4" type="button"  text="配置管理"  image="Menu/Tools.png"     dll="OVIT.BDC.ExchangeData.Tools.dll"  class="OVIT.BDC.ExchangeData.Tools.Activity"   func="ManageConfig"  windowtype="fill" ></Item>
        </Bar>
