        </Bar>
        <Bar id="" index="1" text="其它" >
          <Item id="1" index="1" type="button"  text="重复上报处理"  image="Menu/Tools.png"    dll="OVIT.BDC.ExchangeData.Tools.dll"  class="OVIT.BDC.ExchangeData.Tools.Activity"   func="HandleOtherCFSB"  windowtype="fill" ></Item>
