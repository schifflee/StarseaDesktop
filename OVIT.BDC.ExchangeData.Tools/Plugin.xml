  <Description>不动产数据上报工具</Description>
  <UI>
    <Ribbon>
      <Tab id="" index="1" text="查询">
        <Bar id="" index="1" text="上报情况" >
          <Item id="1" index="1" type="button"  text="日上报"  image="Menu/Calender.png"     dll="OVIT.BDC.ExchangeData.Tools.dll"  class="OVIT.BDC.ExchangeData.Tools.Activity"   func="ShowDayUpload"  windowtype="fill" ></Item>
          <Item id="2" index="2" type="button"  text="周上报"  image="Menu/Calender.png"     dll="OVIT.BDC.ExchangeData.Tools.dll"  class="OVIT.BDC.ExchangeData.Tools.Activity"   func="ShowWeekUpload"  windowtype="fill" ></Item>
